{
  "name": "adecor",
  "version": "0.0.1",
  "description": "Front-end document generation tools",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node-dev' src/index.ts",
    "build": "rimraf lib && tsc"
  },
  "bin": {
    "adecor": "bin/index.ts"
  },
  "dependencies": {
    "@babel/generator": "^7.9.6",
    "@babel/parser": "^7.9.6",
    "@babel/traverse": "^7.9.6",
    "@babel/types": "^7.9.6"
  },
  "devDependencies": {
    "@types/node": "^13.13.4",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.1.0",
    "nodemon": "^2.0.3",
    "rimraf": "^3.0.2",
    "ts-node": "^8.9.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "lib",
    "README.md",
    "package.json"
  ],
  "repository": "git@github.com:shayeLee/fe-doc.git",
  "author": "shayeLee <shaye_web@163.com>",
  "license": "MIT"
}
